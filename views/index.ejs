<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Formularz</title>
</head>
<body>
    <form action="/Studenci" method="POST">
        <input type="number" placeholder="album" name="album">
        <input type="text" placeholder="Imie Studenta" name="imie">
        <input type="text" placeholder="Nazwisko Studenta" name="nazwisko">
        <button type="text">Submit</button>
      </form>
      <table>
        </tr>
        <% for(var i = 0; i < Studenci.length; i++) {%>
          <tr>
              <th><%= Studenci[i].album %></th>
              <th><%= Studenci[i].imie %></th>
              <th><%= Studenci[i].nazwisko %></th>
              <th><button>Usu≈Ñ</button></th>
          </tr>
          <% } %>
    </table>
</body>
</html>

